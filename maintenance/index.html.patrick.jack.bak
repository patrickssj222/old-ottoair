  <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">Search Result</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="flight-card">

  <!-- Jack 2018-09-28 -->
      <script type="text/javascript">
        // Load the airport dropdowns from db
          $('#flying_from').load('http://localhost/ottof/dashboard/common/airport/render_dropdown_options');
          $('#flying_to').load('http://localhost/ottof/dashboard/common/airport/render_dropdown_options');

        // Search modal
          $('#search-now').click(function(){
            $.ajax({
                url: 'dashboard/api/estimate',
                method: 'POST',
                data: $('#flight-estimate').serialize(),
                dataType: 'json',
                beforeSend: function(){
                    // Add button spin animation
                    $('#search-now').html('<i class="fa fa-spin fa-spinner"></i> ' + $('#search-now').html());
                },
                complete: function(){
                    // Remove button spin animation
                    $('#search-now i').remove();
                },
                success: function($data){
                    // console.log($data);

                    $html = '';

                    for (var $i = 0; $i < $data.length; $i ++) {

                        $html += '<div class="card">';
                        $html += '  <div class="row card-body">';

                        $html += '      <div class="col-md-9 flight-result">';
                        $html += '        <div class="row flight-info">';
                        $html += '          <div class="col-3 flight-company">';
                        $html += '            <div class="row element-center">';
                        $html += '              <img src="img/air_canada_logo.png" height="50" width="50">';
                        $html += '            </div>';
                        $html += '            <div class="row element-center">';
                        $html += '              Air Canada';
                        $html += '            </div>';
                        $html += '          </div>';
                        $html += '          <div class="col-9 flight-detail">';
                        $html += '            <div class="flight-time">';
                        $html += '              <div class="row element-center">';
                        $html += '                <div class="col-5">' + $data[$i]['from_dTime'] + '</div>';
                        $html += '                <div class="col-2"><i class="fas fa-arrow-right"></i></div>';
                        $html += '                <div class="col-5">' + $data[$i]['from_aTime'] + '</div>';
                        $html += '              </div>';
                        $html += '            </div>';
                        $html += '            <div class="flight-time">';
                        $html += '              <div class="row element-center">';
                        $html += '                <div class="col-5">' + $data[$i]['city_from'] + '(' + $data[$i]['fly_from'] + ')' + '</div>';
                        $html += '                <div class="col-2"></div>';
                        $html += '                <div class="col-5">' + $data[$i]['city_to'] + '(' + $data[$i]['fly_to'] + ')' + '</div>';
                        $html += '              </div>';
                        $html += '            </div>';
                        $html += '          </div>';
                        $html += '        </div>';

                        $html += '        <div class="row flight-info">';
                        $html += '          <div class="col-3 flight-company">';
                        $html += '            <div class="row element-center">';
                        $html += '              <img src="img/air_canada_logo.png" height="50" width="50">';
                        $html += '            </div>';
                        $html += '            <div class="row element-center">';
                        $html += '              Air Canada';
                        $html += '            </div>';
                        $html += '          </div>';
                        $html += '          <div class="col-9 flight-detail">';
                        $html += '            <div class="flight-time">';
                        $html += '              <div class="row element-center">';
                        $html += '                <div class="col-5">' + $data[$i]['back_dTime'] + '</div>';
                        $html += '                <div class="col-2"><i class="fas fa-arrow-left"></i></div>';
                        $html += '                <div class="col-5">' + $data[$i]['back_aTime'] + '</div>';
                        $html += '              </div>';
                        $html += '            </div>';
                        $html += '            <div class="flight-time">';
                        $html += '              <div class="row element-center">';
                        $html += '                <div class="col-5">' + $data[$i]['city_to'] + '(' + $data[$i]['fly_to'] + ')' + '</div>';
                        $html += '                <div class="col-2"></div>';
                        $html += '                <div class="col-5">' + $data[$i]['city_from'] + '(' + $data[$i]['fly_from'] + ')' + '</div>';
                        $html += '              </div>';
                        $html += '            </div>';
                        $html += '          </div>';
                        $html += '        </div>';

                        $html += '      </div>';

                        $html += '    <div class="col-md-3 flight-purchase ">';
                        $html += '      <div class="row flight-price element-center">$' + $data[$i]['price'] + '</div>';
                        $html += '      <div class="row">';
                        $html += '        <a style="margin:auto" href="dashboard/api/estimate/select/' + $data[$i]['id'] + '" class="btn btn-primary">Purchase Now</a>';
                        $html += '      </div>';
                        $html += '    </div>';
                        $html += '  </div>';
                        $html += '</div>';
                    }

                    // Populate modal
                    $('#exampleModalCenter .modal-body .flight-card').html($html);

                    // Open modal
                    $('#search-now-actual').click();
                }
            });
          });
      </script>

          </div>
        </div>
      </div>

    </div>
